<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.18//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.18.dtd">
<benchmark
    tool="vampire"
    memlimit="16 GB"
    cpuCores="1">

<rundefinition>
    <option name="--input_syntax">tptp</option>
    <option name="-i">5000</option>
    <option name="-si">on</option>
    <option name="-rtra">on</option>
    <option name="--random_seed">12345</option>
    <option name="--sample_strategy">/home/mhajdu/vampire/samplers/samplerFOL.smp</option>
</rundefinition>

<tasks>
    <include>/data/benchmarks/TPTP/Problems/*/*.p</include>
</tasks>

<columns>
    <column title="szs-status">szs-status</column>
    <column title="instruction-count">instruction-count</column>
</columns>

</benchmark>
<!-- vim:fenc=utf-8 -->
