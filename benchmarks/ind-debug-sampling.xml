<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.18//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.18.dtd">
<benchmark
    tool="vampire"
    memlimit="16 GB"
    cpuCores="1">

<rundefinition>
    <option name="--input_syntax">smtlib2</option>
    <option name="-i">5000</option>
    <option name="-si">on</option>
    <option name="-rtra">on</option>
    <option name="--random_seed">12345</option>
    <option name="--sample_strategy">/home/mhajdu/vampire/samplers/samplerIND.smp</option>
</rundefinition>

<tasks>
    <include>/home/mhajdu/inductive_benchmarks/benchmarks/flat/*.smt2</include>
    <include>/data/benchmarks/SMT-LIB/flat/UFDTLIA::20172804-Barrett::induction-vmcai2015::*.smt2</include>
    <include>/home/mhajdu/vampire_benchmarks_flat/induction::*.smt2</include>
</tasks>

<columns>
    <column title="szs-status">szs-status</column>
    <column title="instruction-count">instruction-count</column>
</columns>

</benchmark>
